 //Arithmetic Exception
class Main 
{
    public static void main(String args[])
    {
        try {
            int a = 10, b = 0;
            int c = a/b;
            System.out.println ("Division = " + c);
        }
        catch(ArithmeticException e) {
            System.out.println ("Oooops,Can't divide a number by 0");
        }
    }
}
